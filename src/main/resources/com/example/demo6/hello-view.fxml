<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.HelloController">
   <children>
      <Pane prefHeight="300.0" prefWidth="350.0" />
      <Button fx:id="buttonOne" mnemonicParsing="false" onAction="#clickButton1" text="Button1" translateX="50.0" translateY="-130.0" />
      <Button fx:id="buttonTwo" mnemonicParsing="false" onAction="#clickButton2" text="Button2" translateX="150.0" translateY="-130.0" />
      <Button fx:id="buttonThree" mnemonicParsing="false" onAction="#clickButton3" text="Button3" translateX="250.0" translateY="-130.0" />
      <Label fx:id="labelForButtons" alignment="CENTER" maxHeight="100.0" maxWidth="260.0" prefHeight="60.0" prefWidth="171.0" text="Click the Button" textAlignment="CENTER" translateX="50.0" />
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
